{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/home.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/faq",
      "dest": "/faq.html"
    },
    {
      "src": "/careers",
      "dest": "/careers.html"
    },
    {
      "src": "/home",
      "dest": "/home.html"
    },
    {
      "src": "/team",
      "dest": "/team.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    }
  ]
}
